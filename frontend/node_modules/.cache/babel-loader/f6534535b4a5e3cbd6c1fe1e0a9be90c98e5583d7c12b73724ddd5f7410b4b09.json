n:[/*#__PURE__*/_jsx(\"p\",{children:\"\\u0412 \\u043C\\u043E\\u0435\\u0439 \\u043F\\u0440\\u0430\\u043A\\u0442\\u0438\\u043A\\u0435 \\u043A\\u043E\\u0448\\u0435\\u043D\\u0438\\u043B\\u044C \\u043F\\u043E\\u043A\\u0430\\u0437\\u0430\\u043B\\u0430 \\u0441\\u0435\\u0431\\u044F \\u043E\\u0447\\u0435\\u043D\\u044C \\u0445\\u043E\\u0440\\u043E\\u0448\\u043E. \\u041A\\u0430\\u0440\\u043C\\u0438\\u043D, \\u043A\\u043E\\u0442\\u043E\\u0440\\u044B\\u0439 \\u043D\\u0430\\u0445\\u043E\\u0434\\u0438\\u0442\\u044C\\u0441\\u044F \\u0432 \\u0436\\u0443\\u0447\\u043A\\u0430\\u0445, \\u0441\\u0442\\u043E\\u0435\\u043A \\u043A \\u0441\\u0432\\u0435\\u0442\\u0443, \\u043E\\u0431\\u0440\\u0430\\u0437\\u0443\\u0435\\u0442 \\u043F\\u0440\\u043E\\u0447\\u043D\\u044B\\u0439 \\u043B\\u0430\\u043A, \\u0441\\u0432\\u044F\\u0437\\u044B\\u0432\\u0430\\u044F\\u0441\\u044C \\u0441 \\u0441\\u043E\\u043B\\u044F\\u043C\\u0438 \\u043C\\u0435\\u0442\\u0430\\u043B\\u043B\\u043E\\u0432.\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0412 \\u043A\\u043E\\u043D\\u0441\\u043F\\u0435\\u043A\\u0442\\u0435 \\u0435\\u0441\\u0442\\u044C \\u0440\\u0435\\u0446\\u0435\\u043F\\u0442 \\u043A\\u043E\\u0442\\u043E\\u0440\\u044B\\u0439 \\u0445\\u043E\\u0440\\u043E\\u0448\\u043E \\u043F\\u043E\\u0434\\u0445\\u043E\\u0434\\u0438\\u0442 \\u0434\\u043B\\u044F \\u0441\\u0432\\u0435\\u0436\\u0438\\u0445 \\u043B\\u0438\\u0441\\u0442\\u044C\\u0435\\u0432. \\u0412\\u043E\\u0442 \\u0435\\u0449\\u0451 \\u043E\\u0434\\u0438\\u043D \\u0441\\u043F\\u043E\\u0441\\u043E\\u0431 \\u0434\\u043B\\u044F \\u043E\\u043A\\u0440\\u0430\\u0448\\u0438\\u0432\\u0430\\u043D\\u0438\\u044F \\u043A\\u043E\\u0448\\u0435\\u043D\\u0438\\u043B\\u044C\\u044E \\u0441 \\u0438\\u0441\\u043F\\u043E\\u043B\\u044C\\u0437\\u043E\\u0432\\u0430\\u043D\\u0438\\u0435\\u043C \\u0441\\u0443\\u0445\\u0438\\u0445 \\u043B\\u0438\\u0441\\u0442\\u044C\\u0435\\u0432. \\u0418 \\u0432 \\u044D\\u0442\\u043E\\u043C \\u0441\\u043B\\u0443\\u0447\\u0430\\u0435 \\u0445\\u043E\\u043B\\u043E\\u0434\\u043D\\u044B\\u0439 \\u0446\\u0432\\u0435\\u0442 \\u0444\\u0443\\u043A\\u0441\\u0438\\u0438 \\u0441 \\u0436\\u0451\\u043B\\u0442\\u044B\\u043C\\u0438 \\u043B\\u0438\\u0441\\u0442\\u044C\\u044F\\u043C\\u0438 \\u0431\\u0443\\u0434\\u0435\\u0442 \\u0441\\u043C\\u043E\\u0442\\u0440\\u0435\\u0442\\u044C\\u0441\\u044F \\u043D\\u0435 \\u0433\\u0440\\u044F\\u0437\\u043D\\u043E.\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u041E\\u0441\\u043D\\u043E\\u0432\\u043D\\u043E\\u0439 \\u0441\\u043F\\u043E\\u0441\\u043E\\u0431. \\u0422\\u043A\\u0430\\u043D\\u044C \\u043F\\u0440\\u043E\\u0442\\u0440\\u0430\\u0432\\u0438\\u0442\\u044C \\u0430\\u043B\\u044E\\u043C\\u043E\\u043A\\u0430\\u043B\\u0438\\u0435\\u0432\\u044B\\u043C\\u0438 \\u043A\\u0432\\u0430\\u0441\\u0446\\u0430\\u043C\\u0438 20% \\u043E\\u0442 \\u0432\\u0435\\u0441\\u0430 \\u0442\\u043A\\u0430\\u043D\\u0438. \\u041A\\u043E\\u0448\\u0435\\u043D\\u0438\\u043B\\u044C (5 \\u0433\\u0440. \\u043F\\u0440\\u0438 \\u0432\\u0435\\u0441\\u0435 \\u0442\\u043A\\u0430\\u043D\\u0438 \\u043E\\u0442 50-70 \\u0433\\u0440.) \\u0438\\u0437\\u043C\\u0435\\u043B\\u044C\\u0447\\u0438\\u0442\\u044C \\u0438 \\u0437\\u0430\\u043B\\u0438\\u0442\\u044C \\u043A\\u0438\\u043F\\u044F\\u0442\\u043A\\u043E\\u043C (\\u0432\\u0441\\u0451 \\u043F\\u043E \\u043A\\u043E\\u043D\\u0441\\u043F\\u0435\\u043A\\u0442\\u0443). \\u041D\\u0438\\u0436\\u043D\\u0438\\u0435 \\u043B\\u0438\\u0441\\u0442\\u044C\\u044F \\u043D\\u0430 \\u043F\\u043E\\u043B\\u0447\\u0430\\u0441\\u0430 \\u0437\\u0430\\u043C\\u0430\\u0447\\u0438\\u0432\\u0430\\u0435\\u043C \\u0432 \\u0436\\u0435\\u043B\\u0435\\u0437\\u043D\\u043E\\u043C \\u043A\\u0443\\u043F\\u043E\\u0440\\u043E\\u0441\\u0435 0,1-0,5 \\u0433\\u0440. \\u0441 \\u0434\\u043E\\u0431\\u0430\\u0432\\u043B\\u0435\\u043D\\u0438\\u0435 \\u0442\\u0430\\u043D\\u0438\\u043D\\u0430 \\u0447\\u0430\\u044F (\\u043E\\u043A\\u043E\\u043B\\u043E \\u0441\\u0442\\u043E\\u043B\\u043E\\u0432\\u043E\\u0439 \\u043B\\u043E\\u0436\\u043A\\u0438