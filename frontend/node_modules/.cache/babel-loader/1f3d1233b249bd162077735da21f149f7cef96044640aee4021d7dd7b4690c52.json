1\\u0442\\u0435\\u0440-\\u043A\\u043B\\u0430\\u0441\\u0441 \\\"\\u0426\\u0432\\u0435\\u0442\\u043D\\u043E\\u0439 \\u0444\\u043E\\u043D\\\", \\u0441\\u043E\\u0441\\u0442\\u043E\\u044F\\u0449\\u0438\\u0439 \\u0438\\u0437 \\u0434\\u0435\\u0432\\u044F\\u0442\\u0438 \\u0432\\u0438\\u0434\\u0435\\u043E \\u0438 \\u043F\\u043E\\u0434\\u0440\\u043E\\u0431\\u043D\\u044B\\u0445 \\u043A\\u043E\\u043D\\u0441\\u043F\\u0435\\u043A\\u0442\\u043E\\u0432,\",/*#__PURE__*/_jsx(Link,{to:\"/auth\",className:styles.landingPage__link,children:\" \\u0437\\u0430\\u0440\\u0435\\u0433\\u0438\\u0441\\u0442\\u0440\\u0438\\u0440\\u0443\\u0439\\u0442\\u0435\\u0441\\u044C. \"}),\"\\u0414\\u043E\\u0431\\u0430\\u0432\\u044C\\u0442\\u0435 \\u0435\\u0433\\u043E \\u0432 \\u043A\\u043E\\u0440\\u0437\\u0438\\u043D\\u0443 \\u0438 \\u043F\\u043E\\u0441\\u043B\\u0435 \\u043E\\u043F\\u043B\\u0430\\u0442\\u044B \\u0432\\u044B \\u0441\\u043C\\u043E\\u0436\\u0435\\u0442\\u0435 \\u043F\\u043E\\u043B\\u0443\\u0447\\u0438\\u0442\\u044C \\u043F\\u043E\\u043B\\u043D\\u044B\\u0439 \\u0434\\u043E\\u0441\\u0442\\u0443\\u043F \\u043A \\u0432 \\u043B\\u0438\\u0447\\u043D\\u043E\\u043C \\u043A\\u0430\\u0431\\u0438\\u043D\\u0435\\u0442\\u0435.\"]}),/*#__PURE__*/_jsx(Link,{to:\"/promo\",className:styles.landingPage__button,children:\"\\u041F\\u043E\\u0434\\u0440\\u043E\\u0431\\u043D\\u0435\\u0435\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0426\\u0435\\u043D\\u0430 4000 \\u0440\\u0443\\u0431\\u043B\\u0435\\u0439.\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0412\\u043E\\u043B\\u0448\\u0435\\u0431\\u043D\\u0438\\u0447\\u0430\\u0439\\u0442\\u0435! \\u0418 \\u0412\\u0430\\u0448\\u0430 \\u0436\\u0438\\u0437\\u043D\\u044C \\u0441\\u0442\\u0430\\u043D\\u0435\\u0442 \\u0432\\u043E\\u043B\\u0448\\u0435\\u0431\\u0441\\u0442\\u0432\\u043E\\u043C.\"}),/*#__PURE__*/_jsx(\"br\",{})]})]});};export default LandingPage;","map":{"version":3,"names":["React","useContext","styles","Link","Slider","Leaf01","Leaf02","images","AuthContext","jsx","_jsx","jsxs","_jsxs","LandingPage","user","isVIP","groups","includes","className","landingPage","children","titleBlock","titleBlock__textTitle","titleBlock__textCopyright","mainLeaf01","src","alt","landingPage__ecoprint","autoPlay","autoPlayTime","landingPage__process","landingPage__video","muted","loop","type","landingPage__plants","mainLeaf02","landingPage__lets_magic","to","landingPage__link","landingPage__bye","landingPage__button"],"sources":["/home/kiipod/projects/slwrm/frontend/src/pages/LandingPage/LandingPage.jsx"],"sourcesContent":["// LandingPage.jsx\nimport React, { useContext } from 'react';\nimport styles from './LandingPage.module.scss';\nimport { Link } from \"react-router-dom\";\nimport Slider from \"../../components/Slider/Slider\";\nimport Leaf01 from \"../../assets/leaf_1.png\";\nimport Leaf02 from \"../../assets/leaf_2.png\";\nimport images from \"./ImageList\";\nimport { AuthContext } from '../../context/AuthContext';\n\nconst LandingPage = () => {\n  const { user } = useContext(AuthContext);\n  const isVIP = user && user.groups && user.groups.includes('VIP');\n\n  return (\n    <div className={styles.landingPage}>\n      <div className={styles.titleBlock}>\n        <h1 className={styles.titleBlock__textTitle}>ДушуГрею</h1>\n        <h1 className={styles.titleBlock__textCopyright}>© Нина Кольцова</h1>\n        <img className={styles.mainLeaf01} src={Leaf01} alt=\"Leaf image\"/>\n      </div>\n\n      <div className={styles.landingPage__ecoprint}>\n        <h1>Экопринт</h1>\n        <br/>\n        <br/>\n        <p>\n          Экопринт - это метод натурального окрашивания, который позволяет перенести природные цвета растений на ткань\n          или бумагу с использованием солей металлов.\n        </p>\n        <p>\n          Одной из основных особенностей экопринта является его уникальность и индивидуальность. Даже листья с одного и\n          того же растения могут окрашивать ткань по-разному. Это открывает бесконечные возможности для творчества и\n          экспериментов с различными вариантами окрашивания.\n        </p>\n      </div>\n\n      <Slider autoPlay={true} autoPlayTime={4000} images={images}/>\n\n      <div className={styles.landingPage__process}>\n        <h1>Процесс</h1>\n        <br/>\n        <br/>\n        <p>\n          Процесс начинается с подготовки ткани. Ткань предварительно обрабатывается протравой, содержащей соли\n          металлов. Эти соли помогают пигменту из листьев осесть на ткани и закрепиться, обеспечивая стойкость\n          окрашивания.\n        </p>\n        <p>\n          Важно отметить, что каждый мастер может использовать свои собственные рецепты и методы обработки, так как\n          точного рецепта для всех не существует. Сезон сбора листьев, жесткость воды и другие факторы могут влиять на\n          окончательный результат окрашивания.\n        </p>\n      </div>\n\n      <div className={styles.landingPage__video}>\n        <video muted autoPlay loop>\n          <source\n            src=\"/videos/landingPage/Process_zadnik.mp4\"\n            type=\"video/mp4\"\n          />\n          Your browser does not support the video tag.\n          Ваш браузер не поддерживает тег видео.\n        </video>\n      </div>\n\n      <div className={styles.landingPage__plants}>\n        <h1>Растения</h1>\n        <br/>\n        <br/>\n        <p>\n          Растения играют ключевую роль в процессе экопринта. Разные виды растений имеют различные свойства окрашивания,\n          отдавая свои уникальные оттенки и оттиски на ткань. Некоторые растения могут давать яркие и насыщенные цвета,\n          в то время как другие создают более нежные и пастельные оттенки.\n        </p>\n        <p>\n          Например, листья эвкалипта могут создавать оттенки от оливково-зеленого до ярко-оранжевого, а листья ольхи от\n          зеленого до графитового. Это позволяет мастерам экопринта экспериментировать с комбинациями различных растений\n          для создания удивительных эффектов и уникальных цветовых схем. Какие растения выбрать и какие результаты\n          получить - решение остается за мастером экопринта.\n        </p>\n      </div>\n\n      <img className={styles.mainLeaf02} src={Leaf02} alt=\"Leaf image\"/>\n\n      <div className={styles.landingPage__video}>\n        <video muted autoPlay loop>\n          <source\n            src=\"/videos/landingPage/Rasteniya_zadnik.mp4\"\n            type=\"video/mp4\"\n          />\n          Your browser does not support the video tag.\n          Ваш браузер не поддерживает тег видео.\n        </video>\n      </div>\n\n      <div className={styles.landingPage__lets_magic}>\n        <h1>Давайте волшебничать!</h1>\n        <br/>\n        <br/>\n\n        <p>\n          Экопринт предлагает гармоничный подход к окрашиванию тканей в сотрудничестве с природой. Он позволяет\n          сохранить уникальность и неповторимость каждого изделия, созданного в результате этого процесса. Кроме того,\n          такой способ окрашивания является экологически дружественным, поскольку не требует использования химических\n          красителей или загрязняющих веществ.\n        </p>\n        <p>\n          Предлагаю вам самим попробовать этот уникальный творческий эксперимент рука об руку с природой. Давайте вместе\n          волшебничать, если вам кажется, что это сложно и не для вас, то я предлагаю стать смелее. Посмотрите мои\n          <Link to=\"/lessons\" className={styles.landingPage__link}> бесплатные уроки,</Link> и если решитесь – приобретайте мой мастер-класс, в котором я подробно рассказываю все\n          тонкости процесса.\n        </p>\n      </div>\n\n      {!isVIP && (\n        <div className={styles.landingPage__bye}>\n          <h1>Покупайте мастер-класс!</h1>\n          <br/>\n          <br/>\n          <p>\n            Если Вы хотите приобрести мастер-класс \"Цветной фон\",\n            состоящий из девяти видео и подробных конспектов,\n            <Link to=\"/auth\" className={styles.landingPage__link}> зарегистрируйтесь. </Link>\n            Добавьте его в корзину и после оплаты вы сможете получить полный доступ к в личном кабинете.\n          </p>\n          <Link to=\"/promo\" className={styles.landingPage__button}>\n            Подробнее\n          </Link>\n          <p>Цена 4000 рублей.</p>\n          <p>Волшебничайте! И Ваша жизнь станет волшебством.</p>\n          <br/>\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default LandingPage;"],"mappings":"AAAA;AACA,MAAO,CAAAA,KAAK,EAAIC,UAAU,KAAQ,OAAO,CACzC,MAAO,CAAAC,MAAM,KAAM,2BAA2B,CAC9C,OAASC,IAAI,KAAQ,kBAAkB,CACvC,MAAO,CAAAC,MAAM,KAAM,gCAAgC,CACnD,MAAO,CAAAC,MAAM,KAAM,yBAAyB,CAC5C,MAAO,CAAAC,MAAM,KAAM,yBAAyB,CAC5C,MAAO,CAAAC,MAAM,KAAM,aAAa,CAChC,OAASC,WAAW,KAAQ,2BAA2B,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAExD,KAAM,CAAAC,WAAW,CAAGA,CAAA,GAAM,CACxB,KAAM,CAAEC,IAAK,CAAC,CAAGb,UAAU,CAACO,WAAW,CAAC,CACxC,KAAM,CAAAO,KAAK,CAAGD,IAAI,EAAIA,IAAI,CAACE,MAAM,EAAIF,IAAI,CAACE,MAAM,CAACC,QAAQ,CAAC,KAAK,CAAC,CAEhE,mBACEL,KAAA,QAAKM,SAAS,CAAEhB,MAAM,CAACiB,WAAY,CAAAC,QAAA,eACjCR,KAAA,QAAKM,SAAS,CAAEhB,MAAM,CAACmB,UAAW,CAAAD,QAAA,eAChCV,IAAA,OAAIQ,SAAS,CAAEhB,MAAM,CAACoB,qBAAsB,CAAAF,QAAA,CAAC,kDAAQ,CAAI,CAAC,cAC1DV,IAAA,OAAIQ,SAAS,CAAEhB,MAAM,CAACqB,yBAA0B,CAAAH,QAAA,CAAC,gFAAe,CAAI,CAAC,cACrEV,IAAA,QAAKQ,SAAS,CAAEhB,MAAM,CAACsB,UAAW,CAACC,GAAG,CAAEpB,MAAO,CAACqB,GAAG,CAAC,YAAY,CAAC,CAAC,EAC/D,CAAC,cAENd,KAAA,QAAKM,SAAS,CAAEhB,MAAM,CAACyB,qBAAsB,CAAAP,QAAA,eAC3CV,IAAA,OAAAU,QAAA,CAAI,kDAAQ,CAAI,CAAC,cACjBV,IAAA,QAAI,CAAC,cACLA,IAAA,QAAI,CAAC,cACLA,IAAA,MAAAU,QAAA,CAAG,oyBAGH,CAAG,CAAC,cACJV,IAAA,MAAAU,QAAA,CAAG,24CAIH,CAAG,CAAC,EACD,CAAC,cAENV,IAAA,CAACN,MAAM,EAACwB,QAAQ,CAAE,IAAK,CAACC,YAAY,CAAE,IAAK,CAACtB,MAAM,CAAEA,MAAO,CAAC,CAAC,cAE7DK,KAAA,QAAKM,SAAS,CAAEhB,MAAM,CAAC4B,oBAAqB,CAAAV,QAAA,eAC1CV,IAAA,OAAAU,QAAA,CAAI,4CAAO,CAAI,CAAC,cAChBV,IAAA,QAAI,CAAC,cACLA,IAAA,QAAI,CAAC,cACLA,IAAA,MAAAU,QAAA,CAAG,snCAIH,CAAG,CAAC,cACJV,IAAA,MAAAU,QAAA,CAAG,iyCAIH,CAAG,CAAC,EACD,CAAC,cAENV,IAAA,QAAKQ,SAAS,CAAEh